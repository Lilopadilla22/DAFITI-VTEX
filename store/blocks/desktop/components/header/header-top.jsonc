    "title": "anuncios top header container de envios icono",
    "children": ["logo#header__anuncio--envios"],
    "props": {
      "width": "20%"
    }
  },

  "logo#header__anuncio--envios": {
    "title": "icono envios",
    "props": {
      "title": "camion de envios",
      "href": "/",
      "url": "assets/svg/truck.svg",
      "width": "11",
      "blockClass": "truck-icon-first"
    }
  },

  "flex-layout.col#anuncios_envios-text": {
    "title": "anuncios top header container de envios texto",    
    "children": ["rich-text#header__anuncio--envios"],
    "props": {
      "width": "80%"
    }
