{
  "flex-layout.row#header__anuncio": {
    "title": "Row de anuncios top header",
    "children": ["flex-layout.col#header__anuncio"],
    "props": {
      "blockClass": "header__anuncios_row",
      "horizontalAlign": "center"
    }
  },

  "flex-layout.col#header__anuncio": {
    "title": "Col de anuncios top header",
    "children": ["flex-layout.row#header__anuncio__anuncios"],
    "props": {
      "width": "70%",
      "blockClass": "header__anuncios_col"
    }
  },

  "flex-layout.row#header__anuncio__anuncios": {
    "title": "container de anuncios",
    "children": [
      "flex-layout.col#header__anuncio--envios",
      "flex-layout.col#header__anuncio--cambios",
      "flex-layout.col#header__anuncio--sigue",
      "flex-layout.col#header__anuncio--nuevo",
      "flex-layout.row#header__anuncio--conocenos"
    ],
    "props": {
      "blockClass": "header__anuncios",
      "horizontalAlign": "center"
    }
  },

  "flex-layout.col#header__anuncio--envios": {
    "title": "container anuncios top header de envios",
