    "disclosure-layout#product__detail__phone__detalles": {
        "title": "container disclosure phone detalle",
        "children": [
            "disclosure-trigger#product__detail__phone__detalles",
            "disclosure-content#product__detail__phone__detalles"
        ],
        "props": {
            "blockClass": "container__disclosure__detalle"
        }
    },

    "disclosure-trigger#product__detail__phone__detalles": {
        "title": "container disclosure text phone detalle",
        "children": [
            "rich-text#product__detail__phone__detalles"
            // "disclosure-state-indicator"
        ],
        "props": {
            "blockClass": "container__text__tigger"
        }
    },

    "rich-text#product__detail__phone__detalles": {
        "title": "disclosure text phone detaller",
        "props": {
            "text": "Detalles del producto",
            "blockClass": "text__tigger__phone"
        }
    },

    "disclosure-content#product__detail__phone__detalles": {
        "title": "container disclosure contenido de detalle phone",
        "children": [
            "flex-layout.row#disclosure__product__detail__phone__detalles"
        ],
        "props": {
            "horizontalAlign": "center",
            "blockClass": "content__disclosure__phone"
        }
    },

    "flex-layout.row#disclosure__product__detail__phone__detalles": {
        "title": "container descripcion del producto phone",
        "children": [
            "product-description"
        ],
        "props": {
            "blockClass": "container__content__phone"
        }
    }
